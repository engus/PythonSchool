   "source": [
    "def gcd_subst(a, b):\n",
    "    if a == b: \n",
    "        return a\n",
    "    else:\n",
    "        max, min = a if a > b else b, a if a < b else b\n",
    "        return lcd(max - min, min)"
   ]
